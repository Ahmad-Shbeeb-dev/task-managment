{
  "name": "@acme/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "dev": "expo start --clear --go",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "prod:android": "eas build --local --platform android --output ./build/build-$(date +%s).apk",
    "prod:ios": "eas build --local --platform ios",
    "deploy:android": "eas build --platform android",
    "deploy:ios": "eas build --platform android",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo-google-fonts/titan-one": "^0.2.3",
    "@expo/metro-config": "^0.10.7",
    "@gluestack-style/react": "^1.0.45",
    "@gluestack-ui/config": "^1.1.2",
    "@gluestack-ui/themed": "^1.1.3",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.38.4",
    "@trpc/react-query": "^10.38.4",
    "@trpc/server": "^10.38.4",
    "date-fns": "^2.30.0",
    "expo": "^49.0.7",
    "expo-asset": "~8.10.1",
    "expo-build-properties": "~0.8.3",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.12",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-image": "~1.3.5",
    "expo-linking": "~5.0.2",
    "expo-notifications": "~0.20.1",
    "expo-router": "2.0.0",
    "expo-screen-orientation": "~6.0.6",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "nanoid": "^5.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "react-native-svg": "13.9.0",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@acme/api": "^0.1.0",
    "@acme/eslint-config": "^0.2.0",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.10",
    "@expo/config-plugins": "~7.2.2",
    "@types/babel__core": "^7.20.1",
    "@types/react": "^18.2.20",
    "eslint": "^8.49.0",
    "react-native-svg-transformer": "^1.3.0",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/react"
    ],
    "ignorePatterns": [
      "expo-plugins/**"
    ]
  }
}
